{
  "name": "vue3-mycalendar",
  "description": "A calendar and scheduling package for Vue 3",
  "version": "0.1.10",
  "main": "dist/vue3-mycalendar.umd.js",
  "module": "dist/vue3-mycalendar.es.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/vue3-mycalendar.es.js",
      "require": "./dist/vue3-mycalendar.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "dist/main.d.ts",
  "files": [
    "dist",
    "dist/style.css", 
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "javascript",
    "typescript",
    "component",
    "calender",
    "date",
    "iosselect"
  ],
  "dependencies": {
    "vue": "^3.5.12"
  },
  "peerDependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "vue-tsc": "^2.1.6"
  }
}
